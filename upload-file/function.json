{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "storage",
      "type": "blob",
      "direction": "out",
      "path": "upload-files/{filename}",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
